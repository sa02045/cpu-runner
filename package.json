{
  "name": "cpu-runner",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:win-ia32": "electron-builder --win --ia32",
    "build:win-x64": "electron-builder --win --x64",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sa02045/cpu-runner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sa02045/cpu-runner/issues"
  },
  "homepage": "https://github.com/sa02045/cpu-runner#readme",
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13"
  },
  "dependencies": {
    "os-utils": "^0.0.14",
    "sharp": "^0.30.1"
  },
  "build": {
    "appId": "cpurunner",
    "productName": "cpu-runner",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": true
    }
  }
}
